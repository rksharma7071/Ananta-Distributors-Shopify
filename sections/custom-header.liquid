      function toggleMenu_custom() {
        var custom_menu = document.getElementById('custom_menu');
        var close = document.getElementById('close');
        var headerMenu = document.getElementById('header_menu');
    
        custom_menu.style.display = 'none';
        close.style.display = 'block';
        headerMenu.style.display = 'flex';
        headerMenu.style.flexDirection = 'column';
      }
    
      function hideMenu_custom() {
        var custom_menu = document.getElementById('custom_menu');
        var close = document.getElementById('close');
        var headerMenu = document.getElementById('header_menu');
    
        custom_menu.style.display = 'block';
        close.style.display = 'none';
        headerMenu.style.display = 'none';
        
      }
    
      function demo(){
        document.getElementById('inner_width').innerText = "Window Inner width: "+window.innerWidth+ "px"
        console.clear()
        console.log(window.innerWidth)
        setTimeout(demo, 100);
        return window.innerWidth
      }
      console.log(demo())
    
