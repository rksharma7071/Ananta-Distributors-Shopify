    <header>
      <div class="header_logo" style="max-width: {{ section.settings.logo_width | plus: 0 }}px;">
        {%- if section.settings.logo != blank -%}
          {%- liquid
            assign logo_width = section.settings.logo_width
            if section.settings.logo.width < logo_width
              assign logo_width = section.settings.logo.width
            endif
            assign alt_text = section.settings.logo.alt | default: shop.name
          -%}
          {% render 'image',
            image: section.settings.logo,
            src_width: logo_width,
            srcset_2x: true,
            lazy_load: false,
            alt_text: alt_text
          %}
        {% else %}
          <span class="logo__text h2 text-theme-text m-0">
            {%- if section.settings.logo_text != blank -%}
              {{- section.settings.logo_text -}}
            {%- else -%}
              {{ shop.name }}{%- endif -%}
          </span>
        {% endif %}
    
      </div>
    {% assign main_menu_handle = 'main-menu' %}
    {% assign main_menu = linklists[main_menu_handle] %}
    {% if main_menu != empty %}
    <div class="header_menu" id="header_menu">
      <ul class="level-1">
          {% for link in section.settings.menu.links %}
        <li class="menu-link">
          <a href="{{ link.url }}">{{ link.title }}</a>
    
          {% if link.links.size > 0 %}
            <ul class="menu dropdown-child">
              {% for child_link in link.links %}
                <li class="menu-link">
                  <a href="{{ child_link.url }}">{{ child_link.title }}</a>
    
                  {% if child_link.links.size > 0 %}
                    <ul class="menu dropdown-grandchild">
                      {% for grandchild_link in child_link.links %}
                        <li class="menu-link">
                          <a href="{{ grandchild_link.url }}">{{ grandchild_link.title }}</a>
                        </li>
                      {% endfor %}
                    </ul>
                  {% endif %}
                </li>
              {% endfor %}
            </ul>
          {% endif %}
        </li>
      {% endfor %}
      </ul>
    </div>
    {% endif %}
    
      <div class="header_icons">
        
        <ul>
          {% if section.blocks.size > 0 %}
            {% for block in section.blocks %}
            <li>
              <a href="{{ block.settings.icon_url }}"><img src="{{ block.settings.icon_image | img_url }}" alt=""></a>
            </li>
            {% endfor %}
          {% endif %}
          
          <li class="menu">
            <a
              href="#"
              id="custom_menu"
              onclick="toggleMenu_custom()"><img src="https://cdn.shopify.com/s/files/1/0848/9363/0773/files/menu.png" alt=""></a>
          </li>
          <li class="close">
            <a
              href="#"
              id="close"
              onclick="hideMenu_custom()"><img src="https://cdn.shopify.com/s/files/1/0848/9363/0773/files/close.png" alt=""></a>
          </li>
        </ul>
      </div>
    </header>
    
    
    
